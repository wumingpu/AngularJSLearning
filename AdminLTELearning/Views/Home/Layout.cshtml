
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>BaiOps</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="~/bowser_compontents/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/bowser_compontents/Font-Awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/bowser_compontents/AdminLTE/css/AdminLTE.min.css" rel="stylesheet" />
    <!--皮肤-->
    <link href="~/bowser_compontents/AdminLTE/css/skins/skin-blue.min.css" rel="stylesheet" />
    <link href="~/bowser_compontents/angular-loading-bar/build/loading-bar.min.css" rel="stylesheet" />
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper" ng-app="BaiOpsApp">
        <header class="main-header">
            <!-- Logo -->
            <a href="index2.html" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini">Bai</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Bai</b>Ops</span>
            </a>
            <!-- Header Navbar -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- User Account Menu -->
                        <li class="dropdown user user-menu">
                            <!-- Menu Toggle Button -->
                            <a href="#">
                                <span class="hidden-xs">Alexander Pierce</span>
                            </a>
                        </li>
                        <!-- Control Sidebar Toggle Button -->
                        <li>
                            <a href="#" data-toggle="control-sidebar">Exit</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <aside class="main-sidebar">
            <section class="sidebar">
                <ul class="sidebar-menu">
                    <!-- Optionally, you can add icons to the links -->
                    <li class="active" ui-><a ui-sref="Index" ui-sref-active="active"><i class="fa fa-link"></i> <span>Index</span></a></li>
                    <li><a ui-sref="Index2" ui-sref-active="active"><i class="fa fa-link"></i> <span>Index2</span></a></li>
                    <li><a href="#"><i class="fa fa-link"></i> <span>Index3</span></a></li>
                    <li><a href="#"><i class="fa fa-link"></i> <span>Index4</span></a></li>
                    <li><a href="#"><i class="fa fa-link"></i> <span>Index5</span></a></li>
                    @*<li class="treeview">
                            <a href="#">
                                <i class="fa fa-link"></i> <span>Multilevel</span>
                                <span class="pull-right-container">
                                    <i class="fa fa-angle-left pull-right"></i>
                                </span>
                            </a>
                            <ul class="treeview-menu">
                                <li><a href="#">Link in level 2</a></li>
                                <li><a href="#">Link in level 2</a></li>
                            </ul>
                        </li>*@
                </ul>
            </section>
        </aside>
        <div ui-view class="content-wrapper" id="mainFrame">

        </div>
        <!-- /.content-wrapper -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- To the right -->
            <div class="pull-right hidden-xs">
                Anything you want
            </div>
            <!-- Default to the left -->
            <strong>Copyright &copy; 2017 <a href="#">Company</a>.</strong> All rights reserved.
        </footer>
    </div>
    <script src="~/bowser_compontents/jQuery/jquery-2.2.3.min.js"></script>
    <script src="~/bowser_compontents/Angular/angular.min.js"></script>
    <script src="~/bowser_compontents/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/bowser_compontents/AdminLTE/js/app.min.js"></script>
    <script src="~/bowser_compontents/ui-router/release/angular-ui-router.min.js"></script>
    <script src="~/bowser_compontents/angular-loading-bar/build/loading-bar.min.js"></script>
    <script src="~/bowser_compontents/ocLazyLoad/dist/ocLazyLoad.min.js"></script>
    <script type="text/javascript">
        angular.module('BaiOpsApp', ['ui.router', 'oc.lazyLoad']).config(['$stateProvider', '$urlRouterProvider', '$ocLazyLoadProvider', function ($stateProvider, $urlRouterProvider, $ocLazyLoadProvider) {
            $ocLazyLoadProvider.config({
                debug: true,
                events: true,
            });

            $stateProvider.state('Index', {
                url: '/Home/FirstPage',
                templateUrl: '/Home/FirstPage',
                controller: 'IndexCtrl',
                resolve: {
                    loadMyFiles: function ($ocLazyLoad) {
                        return $ocLazyLoad.load({
                            name: 'BaiOpsApp',
                            files: [
                            '/Scripts/PageScript/FirstPage.js',
                            ]
                        })
                    }
                }
            })
            .state('Index2', {
                url: '/Home/SecondPage',
                templateUrl: '/Home/SecondPage',
                controller: 'SecondCtrl'
            });

            $urlRouterProvider.otherwise('/Home/FirstPage');
        }]);
    </script>
</body>
</html>
